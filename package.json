{
  "name": "automatic-virtual-atelier",
  "version": "0.1.0",
  "description": "Automatically generate Virtual Atelier shops for installed mods based on separators in Mod Organizer.",
  "main": "index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "nexe": "tsup && nexe dist/index.js --build --output bin/AVA.exe",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [],
  "author": "Zach Witherow",
  "license": "ISC",
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/ini": "^4.1.1",
    "@types/md5": "^2.3.5",
    "nexe": "4.0.0-rc.6",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "fs-extra": "^11.2.0",
    "ini": "^5.0.0",
    "md5": "^2.3.0",
    "ora": "^8.1.0",
    "yaml": "^2.5.1"
  },
  "tsup": {
    "entry": [
      "src"
    ]
  },
  "splitting": false,
  "sourcemap": true,
  "clean": true
}
