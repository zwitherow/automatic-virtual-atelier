{
  "name": "automatic-virtual-atelier",
  "version": "0.1.0",
  "description": "Automatically generate Virtual Atelier shops for installed mods based on separators in Mod Organizer.",
  "main": "index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "build": "tsup",
    "start": "cross-env NODE_ENV=development node dist/index.js",
    "nexe": "tsup && nexe dist/index.js --build --output bin/AVA.exe",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [],
  "author": "Zach Witherow",
  "license": "ISC",
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/ini": "^4.1.1",
    "@types/md5": "^2.3.5",
    "cross-env": "^7.0.3",
    "nexe": "4.0.0-rc.6",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "human-readable": "^0.2.1",
    "ini": "^5.0.0",
    "md5": "^2.3.0",
    "ora": "^8.1.0",
    "pretty-bytes": "^6.1.1",
    "yaml": "^2.5.1"
  }
}
